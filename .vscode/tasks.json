{
    "tasks": [
        {
            "type": "shell",
            "label": "Make Debug",
            "windows": {
                "command": "mingw32-make -f prj/gcc/Makefile debug=true"
            },
            "linux": {
                "command": "make -f prj/gcc/Makefile debug=true"
            },
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
